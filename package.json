{
  "name": "eztvt-server",
  "version": "1.0.0",
  "description": "EzTvT Server",
  "main": "server.js",
  "repository": "https://github.com/claeri27/eztvt-server.git",
  "author": "Christopher Laeri",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "nodemon src/server.js",
    "db:start": "cd ./src/ && sequelize db:migrate && sequelize db:seed:all",
    "db:undo": "cd ./src/ && sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "db:undo:one": "cd ./src/ && sequelize db:migrate:undo",
    "lint": "eslint --fix . && echo 'Linting completed.'"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.10.1",
    "sequelize-cli": "^5.5.0",
    "uuid": "^3.3.2"
  }
}
